<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.isomorphism.mapper.FriendRequestMapperCustom">

    <select id="queryNewFriendList"
            parameterType="map"
            resultType="org.isomorphism.pojo.vo.NewFriendsVO">
        select
            *
        from
            friend_request fr
        left join
            users u
        on
            fr.my_id = u.id
        order by
            fr.request_time
        desc
    </select>

</mapper>
